{
  "name": "newq",
  "description": "A distributed, modular, functional, adapter-based, type-safe queue system that works flawlessly in multi-replica and multi-worker setups.",
  "version": "0.0.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./adapters/*": {
      "require": {
        "types": "./dist/adapters/*.d.cts",
        "default": "./dist/adapters/*.cjs"
      },
      "import": {
        "types": "./dist/adapters/*.d.ts",
        "default": "./dist/adapters/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "license": "MIT",
  "scripts": {
    "build": "bunup",
    "dev": "bunup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage",
    "release": "bumpp --commit --push --tag --access public",
    "publish:ci": "bun publish --access public --no-git-checks"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/bun": "^1.3.2",
    "bumpp": "^10.2.0",
    "bunup": "^0.15.14",
    "typescript": "^5.9.3",
    "drizzle-orm": "^0.28.0",
    "@libsql/client": "^0.3.0",
    "mongodb": "^6.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0",
    "drizzle-orm": ">=0.28.0",
    "@libsql/client": ">=0.3.0",
    "mongodb": ">=6.0.0",
    "redis": ">=5.9.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "drizzle-orm": {
      "optional": true
    },
    "@libsql/client": {
      "optional": true
    },
    "mongodb": {
      "optional": true
    },
    "redis": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highxai/newq.git"
  },
  "bugs": {
    "url": "https://github.com/highxai/newq/issues"
  },
  "homepage": "https://newq.highx.dev",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "queue",
    "message-queue",
    "task-runner",
    "drizzle",
    "mongodb",
    "redis",
    "background-jobs",
    "task-queue",
    "workflow"
  ]
}
